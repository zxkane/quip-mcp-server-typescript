{
  "name": "quip-mcp-server-example-client",
  "version": "1.0.0",
  "description": "Example client for testing quip-mcp-server",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "start:stdio": "node build/index.js",
    "start:http": "node build/index.js --http",
    "start:http:3000": "node build/index.js --http --port 3000",
    "start:http:8080": "node build/index.js --http --port 8080",
    "dev": "ts-node --esm src/index.ts",
    "dev:http": "ts-node --esm src/index.ts --http"
  },
  "keywords": [
    "quip",
    "mcp",
    "client"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.2",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
